<h1>Thorberry</h1>
<%= form_for :login, url: auth_path do |f| %>
<p>
  <%= f.label "Username:" %>
  <%= f.text_field :user %>
</p>
<p>
  <%= f.label "Password:" %>
  <%= f.text_field :pwd %>
</p>
<p>
  <%= f.submit "Log In" %>
</p>
<% end %>

<% if flash[:notice] then %>
<p>
  <%= flash[:notice] %>
</p>
<% end %>
  
